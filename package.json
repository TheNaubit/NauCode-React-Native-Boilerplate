{
  "name": "naucodereactnativeboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "install-pods": "yarn && cd ./ios && npx pod-install && cd ..",
    "install-pods-m1": "echo 'If you had not, install ffi for intel with: sudo arch -x86_64 gem install ffi before running this command!' && cd ./ios && arch -x86_64 npx pod-install && cd ..",
    "android-dev-device": "adb reverse tcp:8081 tcp:8081 && npx react-native run-android",
    "generate-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "open-android-debug": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "react": "17.0.2",
    "react-native": "0.64.1",
    "react-native-bootsplash": "3.2.3",
    "react-native-calendar-select": "^0.1.2",
    "react-native-dropdown-picker": "^5.1.14",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-navigation": "7.14.0",
    "react-native-offline": "^5.8.0",
    "react-native-push-notification": "7.3.1",
    "react-native-reanimated": "2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.2.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^12.1.1",
    "react-native-switch-pro": "^1.0.5",
    "react-native-typography": "^1.4.1",
    "react-native-vector-icons": "^8.1.0",
    "rn-placeholder": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/runtime": "7.14.0",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
